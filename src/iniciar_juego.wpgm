import wollok.game.*
import config.*

program iniciarJuego {
	//configuro el juego
	game.cellSize(32)
	
	game.height(15)
	game.width(19)

	game.boardGround("./assets/sprites/Tile/backgroundFull.png")

	
	nivel.configuracionInicial()
	
	//inicio el juego
	game.start()
	game.schedule(0,{ => nivel.startOST()})

}